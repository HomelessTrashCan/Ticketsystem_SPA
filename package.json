{
  "name": "ticket-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "start": "set NODE_ENV=production && node src/server/index.js",
    "start:dev": "set NODE_ENV=development && node src/server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run start:dev\"",
    "test": "set NODE_ENV=test && vitest --run",
    "test:watch": "set NODE_ENV=test && vitest",
    "test:coverage": "set NODE_ENV=test && vitest --coverage",
    "test:k6": "k6 run tests/k6.test.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "seed": "set NODE_ENV=development && node src/server/db/seed.mjs",
    "seed:test": "set NODE_ENV=test && node src/server/db/seed.mjs"
  },
  "dependencies": {
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "supertest": "^7.2.2",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.18"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
